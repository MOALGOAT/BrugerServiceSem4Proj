{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "_id": {
      "type": "Guid",
      "description": "User _id."
    },
    "firstName": {
      "type": "string",
      "description": "User first name."
    },
    "lastName": {
      "type": "string",
      "description": "User last name."
    },
    "password": {
      "type": "string",
      "description": "User password."
    },
    "email": {
      "type": "string",
      "description": "User email."
    },
    "address": {
      "type": "string",
      "description": "User address."
    },
    "telephonenumber": {
      "type": "string",
      "description": "User telephonenumber."
    },
    "role": {
      "type": "integer",
      "description": "User role."
    },
    "username": {
      "type": "string",
      "description": "User username."
    }
  },
  "required": [ "_id", "firstName", "lastName", "password", "email", "address", "telephonenumber", "role", "username" ]
}
